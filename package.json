{
  "name": "angular2-calendar",
  "version": "0.1.3",
  "description": "A calendar component that can display events on a month, week or day view",
  "main": "./dist/umd/angular2-calendar.js",
  "style": "./dist/css/angular2-calendar.css",
  "module": "./dist/es2015-module/angular2-calendar.js",
  "typings": "./dist/es2015-module/angular2-calendar.d.ts",
  "scripts": {
    "start": "concurrently --raw \"webpack-dev-server --open\" \"npm run test:watch\"",
    "build:demo": "webpack -p",
    "build:umd": "webpack --config webpack.config.umd.js",
    "build:dist": "npm run build:umd && npm run ngc",
    "build:clean": "rm -rf dist",
    "test": "karma start",
    "test:watch": "karma start --watch",
    "commit": "git-cz",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s",
    "typedoc": "typedoc --options typedoc.json angular2-calendar.ts src/*.ts",
    "gh-pages": "git checkout gh-pages && git merge master && npm run build:demo && npm run typedoc && git add . && git commit -m 'chore: build demo and docs' && git push && git checkout master",
    "prerelease": "npm test",
    "release": "standard-version --first-release && git push --follow-tags origin master && npm run build:dist && npm publish",
    "postrelease": "npm run build:clean && npm run gh-pages",
    "ngc": "ngc -p tsconfig-es2015.json"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/mattlewis92/angular2-calendar.git"
  },
  "keywords": [
    "angular2",
    "calendar"
  ],
  "author": "Matt Lewis",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/mattlewis92/angular2-calendar/issues"
  },
  "homepage": "https://github.com/mattlewis92/angular2-calendar#readme",
  "devDependencies": {
    "@angular/common": "~2.0.0",
    "@angular/compiler": "~2.0.0",
    "@angular/compiler-cli": "~0.6.2",
    "@angular/core": "~2.0.0",
    "@angular/forms": "~2.0.0",
    "@angular/platform-browser": "~2.0.0",
    "@angular/platform-browser-dynamic": "~2.0.0",
    "@angular/platform-server": "~2.0.0",
    "@ng-bootstrap/ng-bootstrap": "~1.0.0-alpha.1",
    "@types/chai": "~3.4.30",
    "@types/core-js": "~0.9.32",
    "@types/jasmine": "~2.2.33",
    "@types/sinon": "~1.16.27",
    "@types/sinon-chai": "~2.7.26",
    "autoprefixer": "~6.4.0",
    "awesome-typescript-loader": "~2.2.4",
    "bootstrap": "~3.3.7",
    "chai": "~3.5.0",
    "commitizen": "~2.8.1",
    "concurrently": "~2.2.0",
    "conventional-changelog": "~1.1.0",
    "conventional-changelog-cli": "~1.2.0",
    "core-js": "~2.4.1",
    "css-loader": "~0.25.0",
    "cz-conventional-changelog": "~1.2.0",
    "extract-text-webpack-plugin": "~2.0.0-beta.4",
    "file-loader": "~0.9.0",
    "font-awesome": "~4.6.3",
    "ghooks": "~1.3.0",
    "imports-loader": "~0.6.5",
    "intl": "~1.2.4",
    "jasmine-core": "~2.5.0",
    "karma": "~1.3.0",
    "karma-coverage": "~1.1.0",
    "karma-jasmine": "~1.0.2",
    "karma-phantomjs-launcher": "~1.0.0",
    "karma-remap-istanbul": "~0.2.0",
    "karma-sourcemap-loader": "~0.3.7",
    "karma-webpack": "~1.8.0",
    "moment": "~2.15.0",
    "node-sass": "~3.10.0",
    "phantomjs-prebuilt": "~2.1.7",
    "postcss-flexibility": "~1.1.0",
    "postcss-loader": "~0.13.0",
    "rxjs": "5.0.0-beta.12",
    "sass-loader": "~4.0.1",
    "sinon": "~1.17.4",
    "sinon-chai": "~2.8.0",
    "sourcemap-istanbul-instrumenter-loader": "~0.2.0",
    "standard-version": "~2.4.0",
    "style-loader": "~0.13.0",
    "stylelint": "~7.3.1",
    "stylelint-config-standard": "~13.0.0",
    "stylelint-webpack-plugin": "~0.3.0",
    "tslint": "~3.15.1",
    "tslint-loader": "~2.1.4",
    "typedoc": "~0.4.3",
    "typescript": "~2.0.0",
    "url-loader": "~0.5.7",
    "validate-commit-msg": "~2.8.0",
    "webpack": "~2.1.0-beta.22",
    "webpack-dev-server": "~2.1.0-beta.4",
    "zone.js": "~0.6.12"
  },
  "peerDependencies": {
    "@angular/core": "^2.0.0-rc.5",
    "@angular/common": "^2.0.0-rc.5",
    "@angular/platform-browser": "^2.0.0-rc.5",
    "moment": "^2.0.0"
  },
  "files": [
    "dist",
    "scss"
  ],
  "config": {
    "ghooks": {
      "commit-msg": "validate-commit-msg"
    },
    "commitizen": {
      "path": "node_modules/cz-conventional-changelog"
    }
  },
  "dependencies": {
    "calendar-utils": "0.0.25"
  }
}
